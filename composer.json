{
    "name": "acme/laravel-scaffold",
    "type": "library",
    "description": "A powerful scaffolding tool for Laravel to generate models, controllers, routes, and more.",
    "license": "MIT",
    "authors": [
        {
            "name": "issafy",
            "email": "skywalker7102pif@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/console": "^11.0 || ^12.0",
        "illuminate/filesystem": "^11.0 || ^12.0",
        "illuminate/support": "^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "Acme\\Scaffold\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Acme\\Scaffold\\ScaffoldServiceProvider"
            ]
        }
    }
}
